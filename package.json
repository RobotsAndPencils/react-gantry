{
  "name": "react-gantry",
  "version": "1.0.0",
  "description": "initial test",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --config config/webpack.config.js",
    "dist": "npx webpack --config config/webpack.config.js --mode production",
    "test": "standard && jest",
    "lint": "standard",
    "install-robits": "npm install --save git+https://github.com/RobotsAndPencils/react-robits.git",
    "eject-robits": "node ./node_modules/react-robits/ejection-script"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobotsAndPencils/react-gantry.git"
  },
  "keywords": [
    "react",
    "starter-kit"
  ],
  "author": "Robots & Pencils",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RobotsAndPencils/react-gantry/issues"
  },
  "homepage": "https://github.com/RobotsAndPencils/react-gantry#readme",
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.5",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.4",
    "mini-css-extract-plugin": "^0.2.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "react-svg-loader": "^2.1.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^1.3.3",
    "standard": "^12.0.1",
    "style-loader": "^0.20.3",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "redux": "^3.7.2",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.3.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.s?css$": "identity-obj-proxy",
      "^.+\\.svg\\?external$": "<rootDir>/config/fileMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "babel-jest",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ]
  }
}
