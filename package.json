{
  "name": "react-gantry",
  "version": "1.0.0",
  "description": "initial test",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --config config/webpack.config.js",
    "dist": "npx webpack --config config/webpack.config.js --mode production",
    "test": "standard && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobotsAndPencils/react-gantry.git"
  },
  "keywords": [
    "react",
    "starter-kit"
  ],
  "author": "Robots & Pencils",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RobotsAndPencils/react-gantry/issues"
  },
  "homepage": "https://github.com/RobotsAndPencils/react-gantry#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "autoprefixer": "^7.2.5",
    "axios-mock-adapter": "^1.14.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.2.0",
    "babel-loader": "^7.1.2",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "mini-css-extract-plugin": "^0.2.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.1",
    "react-svg-loader": "^2.1.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.3",
    "standard": "^11.0.1",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-thunk": "^2.2.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.s?css$": "identity-obj-proxy",
      "^.+\\.svg\\?external$": "<rootDir>/config/fileMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "babel-jest",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ]
  }
}
